 <h1>Nuevo persona</h1>
    <form role="form">
      <div class="form-group">
        <label for="cedula">Cedula</label>
        {{input id='cedula' class="form-control" value=cedula}}
      </div>
      <div class="form-group">
        <label for="nombre">Nombre</label>
        {{input id='nombre' class="form-control" value=nombre}}
      </div>
      <div class="form-group">
        <label for="apellido">apellido</label>
        {{input id='apellido' class="form-control" value=apellido}}
      </div>

      <button type="submit" class="btn btn-default" {{action 'crear'}}>crear</button>

    </form>
